<div class="container">
	<h3> Login </h3>
<%= form_with url:"/login" do |f| %>
	<div>
		<%= f.label :username %>
		<%= f.text_field :username, required: true %>
	</div>
	<div>
		<%= f.label :password %>
		<%= f.password_field :password, required: true %>
	</div>
	<div>
		<%= f.submit "Submit" %>
	</div>
<% end %>
</div>